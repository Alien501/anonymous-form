[run]
source = .
omit =
    */migrations/*
    */tests.py
    */test_*.py
    *_tests.py
    */venv/*
    */env/*
    */virtualenv/*
    manage.py
    */conftest.py
    */settings.py
    */wsgi.py
    */asgi.py
    */apps.py
    */admin.py

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract
    
precision = 2
show_missing = True

[html]
directory = htmlcov

